<Page x:Class="AplicacionEscritorioCore.Vista.Archivos.GenerarExcelProductos"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AplicacionEscritorioCore.Vista.Archivos"
      mc:Ignorable="d" 
      Title="GenerarExcelProductos"

  >

    <StackPanel >
        <TextBlock Margin="8 0 8 8" Text="Ingrese codigo externo de la orden de trabajo" />
        <TextBox Padding="4" Margin="8 0 8 4" Width="400" x:Name="txtCodigoOtl" />
      
        <Button HorizontalAlignment="Right" Margin="8 0 8 0" Padding="16 4" Content="Generar" Click="Button_Click"/>
        <!-- Background="#0d6efd" Foreground="White"  -->
    </StackPanel>
</Page>
