<Page x:Class="AplicacionEscritorioCore.Vista.Archivos.GenerarExcelProductos"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AplicacionEscritorioCore.Vista.Archivos"
      mc:Ignorable="d" 
      Title="GenerarExcelProductos"

  >

    <StackPanel>
        <TextBlock Margin="8 0 8 8" Text="Ingresar Rango de Fecha" />
        <WrapPanel >
            <DatePicker  Margin="8 0 4 0" Name="Fechainicio"/>
            <DatePicker Name="FechaFin"/>

        </WrapPanel>
        <ProgressBar HorizontalAlignment="Left"  Margin="8 5 0 0" Height="15" Width="290"/>


        <Button HorizontalAlignment="Right" Margin="8 5 8 0" Padding="16 4" Content="Generar" Click="Button_Click"/>
     


        <!-- Background="#0d6efd" Foreground="White"  -->
    </StackPanel>
</Page>
